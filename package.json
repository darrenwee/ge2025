{
  "name": "nextjs-kysely",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "20.10.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate:create": "ts-node ./src/scripts/create-migration.ts",
    "db:migrate:up": "ts-node -r ./src/scripts/with-env.ts ./src/scripts/migrate.ts up && npm run db:schema:generate",
    "db:migrate:down": "ts-node -r ./src/scripts/with-env.ts ./src/scripts/migrate.ts down && npm run db:schema:generate",
    "db:schema:generate": "kysely-codegen --out-file ./src/types/kysely.ts",
    "ncu": "pnpm dlx npm-check-updates --interactive --format group",
    "unimported": "pnpm dlx unimported"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@turf/center": "^7.0.0-alpha.113",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.350.0",
    "maplibre-gl": "^4.3.1",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-map-gl": "^7.1.7",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "3.2.5",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
